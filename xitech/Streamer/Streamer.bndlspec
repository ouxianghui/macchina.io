<?xml version="1.0"?>
<bundlespec>
  <manifest>
    <name>XiTech Streamer</name>
    <symbolicName>cn.xitech.iot.streamer</symbolicName>
    <version>1.0.1</version>
    <vendor>XiTech</vendor>
    <copyright>(c) 2013-2023, XiTech</copyright>
    <activator>
      <class>xi::streamer::BundleActivator</class>
      <library>cn.xitech.iot.streamer</library>
    </activator>
    <lazyStart>false</lazyStart>
    <runLevel>400</runLevel>
  </manifest>
  <code>
    ${bin}/*.dll,
    ${bin}/*.pdb,
    bin/${osName}/${osArch}/*.so,
    bin/${osName}/${osArch}/*.dylib
    lib/*.so
  </code>
  <files>
    bundle/*
  </files>
</bundlespec>
